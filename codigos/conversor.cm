void converteDolar(int val){
    int result;
    result = val * 512;
    output(result);
}

void converteReal(int val){
    int result;
    result = val * 17;
    output(result);
}

int main(void){
    int op;
    int valor;
    op = input();
    valor = input();
    if(op == 1){
        converteDolar(valor);
    }else{
        converteReal(valor);
    }
    return 0;
}